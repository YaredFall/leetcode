{
  "name": "leetcode-solutions",
  "version": "1.0.0",
  "description": "My leetcode solutions",
  "scripts": {
    "test": "jest",
    "create": "bash -c 'touch $@.js && touch $@.test.js' bash"
  },
  "author": "YaredFall",
  "license": "ISC",
  "dependencies": {
    "@types/jest": "^29.5.2",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "type": "module",
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    }
  }
}
